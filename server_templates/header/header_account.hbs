<link href="/static/css/header/header_account.css" rel="stylesheet">

<div class="header-account" ng-controller="headerAccount">
  <div class="membership-details">
    <h4 class="membership-message"></h4>
    <button class="membership-renew-link"></button>
  </div>

  <div class="membership-promotions">
    <div class="instant-savings"></div>
    <div class="cash-rewards"></div>
  </div>

  <div class="links-groups" ng-repeat="linksGroup in linksGroups">
    <div class="links-group">
      <h4 class="links-heading"></h4>
      <ul class="links" ng-repeat="link in links">
        <li class="link"></li>
      </ul>
    </div>
  </div>
</div>

<script src="/static/js/header/header_account.js"></script>
